    "supertest": "7.1.4",
    "ts-loader": "9.5.4",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.46.3",
    "unplugin-swc": "1.5.8",
